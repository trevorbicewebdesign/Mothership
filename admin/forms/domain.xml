	<field name="registrar" type="text" default="" readonly="" class="input-xxlarge input-large-text" label="COM_MOTHERSHIP_FIELD_ACCOUNT_REGISTRAR_LABEL" description="COM_MOTHERSHIP_FIELD_ACCOUNT_REGISTRAR_DESC" />
	<field name="dns_provider" type="text" default="" readonly="" class="input-xxlarge input-large-text" label="COM_MOTHERSHIP_FIELD_ACCOUNT_DNS_PROVIDER_LABEL" description="COM_MOTHERSHIP_FIELD_ACCOUNT_DNS_PROVIDER_DESC" />
